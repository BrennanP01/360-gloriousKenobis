{
	"routes": [
	  {
		"route": "/profile",
		"allowedRoles": ["authenticated"]
	  },
	  {
		"route": "/admin/*",
		"allowedRoles": ["administrator"]
	  },
	  {
		"route": "/api/admin",
		"allowedRoles": ["administrator"]
	  },
	  {
		"route": "/login",
		"serve": "/.auth/login/github"
	  },
	  {
		"route": "/logout",
		"serve": "/.auth/logout"
	  }
	]
  }